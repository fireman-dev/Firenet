<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#D1D9E6" /> <corners android:radius="12dp" />
            <padding android:bottom="2dp" android:right="2dp" />
        </shape>
    </item>

    <item android:bottom="2dp" android:right="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" /> <corners android:radius="12dp" />
        </shape>
    </item>

    <item android:bottom="2dp" android:right="2dp" android:top="2dp" android:left="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#E0E5EC" /> <corners android:radius="12dp" />
            <stroke android:width="1dp" android:color="#D1D9E6"/> </shape>
    </item>
</layer-list>